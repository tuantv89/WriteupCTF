def gcd(a, b):
    while b:
        q, r = a.quo_rem(b)
        a, b = b, r
    if a:
        a = a.monic()
    return a


def attack(c1, c2, e, n):
    x = polygen(Zmod(n))
    g1 = (x ^ e + x) - c1
    g2 = (x + 1) ^ e + x + 1 - c2

    return gcd(g1, g2).small_roots()


